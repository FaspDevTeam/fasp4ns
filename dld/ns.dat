%---------------------------------------------------%
AMG_aggregation_type_p     = 1     % 1 Matching | 2 VMB 
AMG_pair_number_p          = 2     % Number of pairs in matching
AMG_strong_coupled_p	   = 0.02  % strong coupled threshold
